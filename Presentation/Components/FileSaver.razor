@using Presentation.Store.File
@using Fluxor

@inject IDispatcher _dispatcher;

<ImageLink
    Large
    OnClick="@HandleSaveClicked" 
    Source="icons/save-icon.png"
    Alt="Save Work to File" />

@code {
    
    private void HandleSaveClicked() => _dispatcher.Dispatch(new FileSaveAction());
    
}